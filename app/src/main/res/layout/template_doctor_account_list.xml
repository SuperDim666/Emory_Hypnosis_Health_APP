<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/MainPage_Background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@color/signInBackgroundColor"
        tools:ignore="ContentDescription">
    </ImageView>

    <RelativeLayout
        android:id="@+id/MainPage_Edit"
        android:layout_width="384.27dp"
        android:layout_height="62.37dp"
        android:layout_marginTop="13.365dp"
        android:layout_marginStart="13.365dp">

        <TextView
            android:id="@+id/MainPage_Edit2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:enabled="true"
            android:layout_centerVertical="true"
            app:fontFamily="@font/font_burgela_bold"
            android:text="Doctor ID:\nAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
            android:textSize="20dp"
            android:textColor="@color/white" />

    </RelativeLayout>

    <ScrollView
        android:id="@+id/MainPage_AudioPage_ScrollView"
        android:layout_width="match_parent"
        android:layout_height="654.885dp"
        android:layout_marginTop="89.1dp">
        <LinearLayout
            android:id="@+id/MainPage_AudioPage_RecordsList"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/MainPage_Account_Button01"
                android:layout_width="384.27dp"
                android:layout_height="62.37dp"
                android:layout_marginTop="27dp"
                android:layout_marginStart="13.365dp"
                android:enabled="true"
                style="@style/Button2"
                android:layout_centerHorizontal="true"
                android:text="@string/text_MainMenu_upload"
                android:textColor="@color/white"
                >
            </ImageView>
            <ImageView
                android:id="@+id/MainPage_Account_Button02"
                android:layout_width="384.27dp"
                android:layout_height="62.37dp"
                android:layout_marginTop="27dp"
                android:layout_marginStart="13.365dp"
                android:enabled="true"
                style="@style/Button2"
                android:layout_centerHorizontal="true"
                android:text="@string/text_MainMenu_upload"
                android:textColor="@color/white"
                >
            </ImageView>
            <ImageView
                android:id="@+id/MainPage_Account_Button03"
                android:layout_width="384.27dp"
                android:layout_height="62.37dp"
                android:layout_marginTop="27dp"
                android:layout_marginStart="13.365dp"
                android:enabled="true"
                style="@style/Button2"
                android:layout_centerHorizontal="true"
                android:text="@string/text_MainMenu_upload"
                android:textColor="@color/white"
                >
            </ImageView>
        </LinearLayout>
    </ScrollView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/MainPage_MenuMask"
        android:layout_width="393.18dp"
        android:layout_height="129.195dp"
        android:layout_marginStart="8.91dp"
        android:layout_marginTop="752.895dp"
        android:alpha="1.0"
        style="@style/MenuMask">

        <ImageView
            android:id="@+id/MainPage_MenuButton1"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="41.49375dp"
            android:layout_marginTop="16.929dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:background="@android:color/transparent"
            android:clickable="false"
            android:src="@drawable/cd_logo_01" />

        <ImageView
            android:id="@+id/MainPage_MenuButton2"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="41.49375dp"
            android:layout_marginTop="16.929dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/MainPage_MenuButton1"
            android:clickable="false"
            android:background="@null"
            android:src="@drawable/account_logo_02"/>

        <ImageView
            android:id="@+id/MainPage_MenuButton3"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_marginStart="41.49375dp"
            android:layout_marginTop="16.929dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/MainPage_MenuButton2"
            android:clickable="false"
            android:background="@null"
            android:src="@drawable/book_logo_01"/>

        <TextView
            android:id="@+id/MainPage_MenuText1"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/MainPage_MenuButton1"
            app:layout_constraintStart_toStartOf="@+id/MainPage_MenuButton1"
            android:text="@string/text_MainMenu_audio"
            android:fontFamily="@font/font_formal"
            android:textSize="19sp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:textColor="@color/white"/>
        <TextView
            android:id="@+id/MainPage_MenuText2"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/MainPage_MenuButton2"
            app:layout_constraintStart_toStartOf="@+id/MainPage_MenuButton2"
            android:text="@string/text_MainMenu_account"
            android:textSize="19sp"
            android:fontFamily="@font/font_formal"
            android:textStyle="bold"
            android:textAlignment="center"
            android:textColor="@color/menu_cyan"/>
        <TextView
            android:id="@+id/MainPage_MenuText3"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/MainPage_MenuButton3"
            app:layout_constraintStart_toStartOf="@+id/MainPage_MenuButton3"
            android:text="@string/text_MainMenu_tutorial"
            android:textSize="19sp"
            android:fontFamily="@font/font_formal"
            android:textStyle="bold"
            android:textAlignment="center"
            android:textColor="@color/white"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/init_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/shadow_background"
        tools:ignore="ContentDescription"
        >
    </ImageView>

    <!--layout_width: PDS.getWidth() * 0.9349635036496f-->
    <!--layout_marginStart: PDS.getWidth() * 0.0325182481752f-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/account_change"
        android:layout_width="384.2699999999856dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="13.3650000000072dp"
        android:layout_centerVertical="true"
        android:background="@drawable/main_page_blue_board"
        tools:ignore="ContentDescription"
        >

        <!--cardBackgroundColor:inputTextWhite-->
        <!--layout_margin: PDS.getWidth() * 0.0194647201946f-->
        <!--cardCornerRadius: PDS.getWidth() * 0.0583941605839f-->
        <!--cardElevation: 0dp-->
        <androidx.cardview.widget.CardView
            android:id="@+id/MainPage_Account_username_mask"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_margin="8dp"
            app:cardBackgroundColor="@color/inputTextWhite"
            app:cardCornerRadius="24dp"
            app:cardElevation="0dp"
            >

            <!--app:passwordToggleEnabled="true"-->
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:passwordToggleEnabled="true"
                app:hintEnabled="false">

                <!--layout_height: PDS.getHeight() * 0.056116722783389f-->
                <!--layout_marginLeft, layout_marginRight, drawablePadding:-->
                <!--              PDS.getWidth() * 0.02919708029197f-->
                <!--textColorHint: inputTextHintWhite-->
                <EditText
                    android:id="@+id/MainPage_doctor_changeUserID"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:inputType="none"
                    android:enabled="false"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/ic_person"
                    android:drawablePadding="12dp"
                    android:hint="New Doctor ID"
                    android:imeOptions="actionDone"
                    android:singleLine="true"
                    android:textColorHint="@color/inputTextHintWhite"
                    android:autofillHints="username" />
            </com.google.android.material.textfield.TextInputLayout>

        </androidx.cardview.widget.CardView>

        <!--layout_height: PDS.getHeight() * 0.0617283950617f-->
        <!--layout_marginTop: PDS.getHeight() * 0.01122334455667789f-->
        <RelativeLayout
            android:id="@+id/MainPage_changeButtonMask"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/MainPage_Account_username_mask">

            <!--layout_width:      PDS.getWidth()  * 0.364963503649635f-->
            <!--layout_height:     PDS.getHeight() * 0.05611672278f-->
            <!--textSize:          PDS.getHeight() * 0.036496350364963503f-->
            <!--layout_marginLeft: PDS.getWidth()  * 0.2849999999999825f-->
            <Button
                android:id="@+id/SignUpControl_button01"
                style="@style/Button1"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginLeft="117.1349999999928075dp"
                app:layout_constraintTop_toTopOf="parent"
                android:alpha="1.0"
                android:enabled="false"
                android:text="REGISTER"
                android:textSize = "15dp"/>

            <!--layout_width:      PDS.getHeight() * 0.05611672278f-->
            <!--layout_height:     PDS.getHeight() * 0.05611672278f-->
            <!--layout_marginLeft: PDS.getWidth() - PDS.getHeight() * 0.102132435465768799f-->
            <Button
                android:id="@+id/audio_board_back"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="320dp"
                android:background="@drawable/ic_account_change_button_return"
                />

        </RelativeLayout>

        <!--layout_marginTop: PDS.getHeight() * 0.005611672278338945f-->


    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>