                android:textStyle="bold"
                android:fontFamily="@font/font_formal"
                android:textColor="@color/audioBoardTextColor"
                android:textSize="17.82sp"
                />
            <TextView
                android:id="@+id/audio_board_time_end"
                android:layout_width="80.19dp"
                android:layout_height="24.999678dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:gravity="center"
                android:text="10:50:45"
                android:textStyle="bold"
                android:fontFamily="@font/font_formal"
                android:textColor="@color/audioBoardTextColor"
                android:textSize="17.82sp"
