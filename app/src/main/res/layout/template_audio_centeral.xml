            android:gravity="center"
            android:text="AAAAAAAAAAAAAAAAAAAAAAAAAA"
            android:textStyle="bold"
            android:fontFamily="@font/font_burgela"
            android:textColor="@color/audioBoardTextColor"
            android:textSize="32.999967sp"
            />
        <SeekBar
            android:id="@+id/music_progress_bar"
            android:layout_width="338.58dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="124.74dp"
            />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="338.58dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="151.47dp"
